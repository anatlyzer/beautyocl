-- @atlcompiler atl2006
-- @path ABCD=/beautyocl.atl.tests/metamodels/ABCD.ecore
-- @path WXYZ=/beautyocl.atl.tests/metamodels/WXYZ.ecore

module intro1;
create OUT : WXYZ from IN : ABCD;

rule model2model {
	from m1 : ABCD!Model (
		if m1.name = 'something' then
			1
		else
			2
		endif > 2	
	)
	to   m2 : WXYZ!Model (
		name <- 'targetModel'
	)
}
